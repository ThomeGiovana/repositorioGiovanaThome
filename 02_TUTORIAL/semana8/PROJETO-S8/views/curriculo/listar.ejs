<%- include("../cabecalho") %>

	<div id="curriculo">
		<% for (const itens of model) { %>

			<p <% if (itens.idCurriculo> 5) { %> style="background-color: #cecece" <% } %>
					<% if (itens.idCurriculo <=5) { %> style="background-color: #ff0" <% } %>>



							<%= itens.idCurriculo %> : <%= itens.item %> - <%= itens.descricao %> |
										<a href="/curriculo/alterar?id=<%= itens.idCurriculo %>">ALTERAR</a> |
										<a href="/curriculo/remover?id=<%= itens.idCurriculo %>">REMOVER</a>
			</p>
			<% } %>
	</div>

	<%- include("../rodape") %>